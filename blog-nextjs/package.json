{
  "name": "blog-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && yarn sitemap && next export",
    "sitemap": "rm -f public/sitemap-0.xml && next-sitemap --config next-sitemap.config.mjs",
    "prebuild": "yarn feed",
    "start": "next start",
    "feed": "npx ts-node --skip-project --esm ./bin/feed.ts",
    "lint": "next lint"
  },
  "dependencies": {
    "feed": "^4.2.2",
    "highlight.js": "^11.6.0",
    "next": "^12.2.5",
    "next-sitemap": "^3.1.11",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "gray-matter": "^4.0.3",
    "postcss": "^8.4.14",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
