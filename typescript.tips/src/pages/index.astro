---
import Layout from '../layouts/Layout.astro';
import Tip from '../components/Tip.astro';
import { readTips } from '../readTips';

const props = readTips().at(10);
if (!props) { throw new Error('Tip not found'); }
---

<Layout title="TypeScript Tips" description="TypeScript Tips | Make bugs impossible. One TypeScript tip at a time." canonical="https://typescript.tips/">
  <Tip {...props} />
</Layout>
